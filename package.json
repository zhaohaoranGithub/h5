    "release:production": "cross-env NODE_ENV=production ENVIRONMENT=production gulp release",
    "release:test": "npm run release:production && scp -P 1666 ./www/forehead.zip  root@203.60.1.45:/www/dev && ssh root@203.60.1.45 -p 1666 'cd /www/dev &&  unzip -o forehead.zip -d ballbet'",
