    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "e2e": "cross-env NODE_ENV=development node test/e2e/runner.js",
    "e2e-dev": "cross-env NODE_ENV=development node test/e2e/runner.js --dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit && npm run e2e-dev",
    "docs": "cross-env BABEL_ENV=docs vuepress dev docs",
    "update:dev": "ssh root@203.60.1.45 -p 1666 'cd /forehead/global-web && git pull origin dev'",
    "docs:build": "cross-env BABEL_ENV=docs vuepress build docs"
