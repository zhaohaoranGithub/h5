    "analysis": "cross-env NODE_ENV=development MODE=analyse npm start",
    "dll:prepare": "npm run dll:prepare:dev & npm run dll:prepare:prd",
    "dll:prepare:dev": "cross-env NODE_ENV=development gulp dll:prepare",
    "dll:prepare:prd": "cross-env NODE_ENV=production gulp dll:prepare",
